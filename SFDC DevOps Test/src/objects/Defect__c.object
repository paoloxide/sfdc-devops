    <validationRules>
        <fullName>Validate_Developer_Comments</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(AND(UPPER(MID( PRIORVALUE(Current_Status__c), 1, 5))&lt;&gt;&quot;FIXED&quot;,UPPER(MID( Current_Status__c , 1, 5))=&quot;FIXED&quot;),AND(UPPER(PRIORVALUE(Current_Status__c))&lt;&gt;&quot;READY TO RETEST&quot;,UPPER(Current_Status__c)=&quot;READY TO RETEST&quot;)),OR(ISNULL(Developer_Comments__c),TRIM(Developer_Comments__c)=&quot;&quot;,TRIM(PRIORVALUE(Developer_Comments__c)) = TRIM(Developer_Comments__c)))</errorConditionFormula>
        <errorMessage>Developer Comments must be updated when Status is fixed or Ready to restest</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Retest_Comments</fullName>
        <active>true</active>
        <description>Used to enforce the user to enter the retest comment after retest</description>
        <errorConditionFormula>AND(OR(ISNULL(Retesting_Comments__c),TRIM(Retesting_Comments__c)=&quot;&quot;,TRIM(PRIORVALUE(Retesting_Comments__c)) = TRIM(Retesting_Comments__c)), OR(AND(UPPER(MID(PRIORVALUE(Current_Status__c),1,9)) &lt;&gt; &quot;RE-OPENED&quot;,UPPER(MID(Current_Status__c,1,9)) = &quot;RE-OPENED&quot;), AND(UPPER(PRIORVALUE(Current_Status__c)) &lt;&gt; &quot;CLOSED - RESOLVED&quot;, UPPER(Current_Status__c) = &quot;CLOSED - RESOLVED&quot;)))</errorConditionFormula>
        <errorMessage>Retest comments must be updated while reopening or closing error location defect</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Retest_Status</fullName>
        <active>true</active>
        <description>Used to enforce the user to enter the Retest status after retest</description>
        <errorConditionFormula>AND(OR(UPPER(Current_Status__c) = &quot;CLOSED - RESOLVED&quot;,UPPER(MID(Current_Status__c,1,9)) = &quot;RE-OPENED&quot;), OR(ISNULL(TEXT(Retest_Status__c)), TEXT(Retest_Status__c) = &quot;&quot;))</errorConditionFormula>
        <errorMessage>Retest status and Retest comment is required when closing or Re-opening a defect</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Target_Completion_Date</fullName>
        <active>true</active>
        <description>Prevent user from saving a past date as Target completion date</description>
        <errorConditionFormula>AND(OR(Current_Status__c =&quot;Developer Assigned&quot;,Current_Status__c=&quot;Fixed&quot;, Current_Status__c =&quot;Fix in Process&quot;),Target_Completion_Date__c &lt;  TODAY())</errorConditionFormula>
        <errorMessage>Target completion date cannot be in past date. Please enter current/future date or clear the value in the field</errorMessage>
    </validationRules>
