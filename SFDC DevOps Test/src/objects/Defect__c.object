        <externalId>false</externalId>
        <formula>if (OR(Current_Status__c =&quot;Developer Assigned&quot;,Current_Status__c=&quot;Fixed&quot;, Current_Status__c =&quot;Fix in Process&quot;),if(Text(Target_Completion_Date__c - TODAY())&lt;&gt;&quot;&quot;,Text(Target_Completion_Date__c - TODAY()) &amp; &quot; Day(s) Left&quot;,&quot;&quot;),&quot;&quot;)</formula>
